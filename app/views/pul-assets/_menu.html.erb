<lux-menu-bar type="main-menu" :menu-items="[
  {name: 'RDF Files', children: [
    {name: 'Archive Description', href: '<%= priarch_path(format: 'rdf', script_name: script_name) %>'},
    {name: 'Series Listings', href: '<%= priseri_path(format: 'rdf', script_name: script_name) %>'},
  ]},
  <% if user_signed_in? %>
    {name: 'Logout <%= current_user.uid %>', href: '<%= destroy_user_session_path(script_name: script_name) %>'},
  <% else %>
    {name: 'Login', href: '<%= new_user_session_path(provider: 'cas', script_name: script_name) %>', method: 'post'},
  <% end %>
]"/>
