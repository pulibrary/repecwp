<%= simple_form_for(@archive) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :handle %>
    <%= f.input :name %>
    <%= f.input :maintainer_email %>
    <%= f.input :description %>
    <%= f.input :url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
